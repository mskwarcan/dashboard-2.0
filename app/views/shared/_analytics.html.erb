	<% if @update.google_done %>
	<div class="col2">
		
		
		<% results = ActiveSupport::JSON.decode(@update.results) %>
		<table class="data" cellspacing=0>
			<tr>
				<td colspan="2"><h3 class="noMargin bold"><%= Time.now.strftime("%B") %> Current Stats</h3></td>
			</tr>
		<tr>
			<td><h3 class="noMargin">Goal Completions</h3></td> <td><%= results[5]["goalCompletionsAll"] %></td>
		</tr>
		<tr>	
			<td><h3 class="noMargin">Visits</h3></td> <td><%= results[4]["visits"] %></td>
		</tr>
		<tr>	
			<td><h3 class="noMargin">Page Views</h3></td> <td><%= results[1]["pageviews"] %></td>
		</tr>
		<tr>
			<td><h3 class="noMargin">New Visits</h3></td> <td><%= results[3]["newVisits"] %></td>
		</tr>
		<tr>
			<td><h3 class="noMargin">Avg. Time on Site</h3></td> <td><%= results[2]["avgTimeOnSite"] %> seconds</td>
		</tr>
		<tr>
			<td><h3 class="noMargin"></h3>Impressions</td> <td><%= results[6]["impressions"] %></td>
		</tr>
		<tr>
			<td><h3 class="noMargin">Ad Clicks</h3></td> <td><%= results[7]["adClicks"] %></td>
		</tr>
		<tr>
			<td><h3 class="noMargin">Click Through Rate</h3></td> <td><%= sprintf("%.2f", results[7]["adClicks"]/results[6]["impressions"]*100) %>%</td>
		</tr>
		</table>
	</div>
	
		<% results_last = ActiveSupport::JSON.decode(@update.last_month) %>
		<% results_two = ActiveSupport::JSON.decode(@update.two_months) %>
		<% results_three = ActiveSupport::JSON.decode(@update.three_months) %>

		<table class="data" cellspacing=0>
			<tr>
				<td colspan="4"><h3 class="noMargin bold"><%= 1.month.ago.strftime("%B") %>-<%= 2.month.ago.strftime("%B") %> Comparision</h3></td>
			</tr>
			<tr>
				<td>Goal Completions</td>
				<td><%= results_last[5]["goalCompletionsAll"] %></td>
				<% avgTimeOnSite = results_last[5]["goalCompletionsAll"] - results_two[5]["goalCompletionsAll"]%>
				<td class="<% if avgTimeOnSite > 0 %> up<% else %>down<% end %>"><%= avgTimeOnSite %></td>
			</tr>
			<tr>
				<td>Page Views</td>
				<td><%= results_last[1]["pageviews"] %></td>
				<% pageviews = results_last[1]["pageviews"] - results_two[1]["pageviews"] %>
				<td class="<% if pageviews > 0 %> up<% else %>down<% end %>"><%= pageviews %></td>
			</tr>
			<tr>
				<td>Visits</td>
				<td><%= results_last[4]["visits"] %></td>
				<% visits = results_last[4]["visits"] - results_two[4]["visits"]%>
				<td class="<% if visits > 0 %> up<% else %>down<% end %>"><%= visits %></td>
			</tr>
			<tr>
				<td>New Visits</td>
				<td><%= results_last[3]["newVisits"] %></td>
				<% newVisits = results_last[3]["newVisits"] - results_two[3]["newVisits"]%>
				<td class="<% if newVisits > 0 %> up<% else %>down<% end %>"><%= newVisits %></td>
			</tr>
			<tr>
				<td>Avg. Time on Site</td>
				<td><%= results_last[2]["avgTimeOnSite"] %> sec</td>
				<% avgTimeOnSite = results_last[2]["avgTimeOnSite"] - results_two[2]["avgTimeOnSite"]%>
				<td class="<% if avgTimeOnSite > 0 %> up<% else %>down<% end %>"><%= avgTimeOnSite %> sec</td>
			</tr>
			<tr>
				<td>Impressions</td>
				<td><%= results_last[6]["impressions"] %></td>
				<% impressions = results_last[6]["impressions"] - results_two[6]["impressions"]%>
				<td class="<% if impressions > 0 %> up<% else %>down<% end %>"><%= impressions %></td>
			</tr>
				<td>Ad Clicks</td>
				<td><%= results_last[7]["adClicks"] %></td>
				<% clicks = results_last[7]["adClicks"] - results_two[7]["adClicks"]%>
				<td class="<% if clicks > 0 %> up<% else %>down<% end %>"><%= clicks %></td>
			</tr>
				<td>Click Through Rate</td>
				<td><%= sprintf("%.2f", results_last[7]["adClicks"]/results_two[6]["impressions"]*100) %>%</td>
				<% ctr = sprintf("%.2f", results_last[7]["adClicks"]/results_two[6]["impressions"]*100) - sprintf("%.2f", results_two[7]["adClicks"]/results_three[6]["impressions"]*100)%>
				<td class="<% if ctr > 0 %> up<% else %>down<% end %>"><%= ctr %>%</td>
			</tr>
		</table>
		
		<table class="data" cellspacing=0>
			<tr>
				<td colspan="4"><h3 class="noMargin bold"><%= 2.month.ago.strftime("%B") %>-<%= 3.month.ago.strftime("%B") %> Comparision</h3></td>
			</tr>
			<tr>
				<td>Goal Completions</td>
				<td><%= results_two.fifth["goalCompletionsAll"] %></td>
				<% avgTimeOnSite = results_two[5]["goalCompletionsAll"] - results_three[5]["goalCompletionsAll"]%>
				<td class="<% if avgTimeOnSite > 0 %> up<% else %>down<% end %>"><%= avgTimeOnSite %></td>
			</tr>
			<tr>
				<td>Page Views</td>
				<td><%= results_two[1]["pageviews"] %></td>
				<% pageviews = results_two[1]["pageviews"] - results_three[1]["pageviews"] %>
				<td class="<% if pageviews > 0 %> up<% else %>down<% end %>"><%= pageviews %></td>
			</tr>
			<tr>
				<td>Visits</td>
				<td><%= results_two[4]["visits"] %></td>
				<% visits = results_two[4]["visits"] - results_three[4]["visits"]%>
				<td class="<% if visits > 0 %> up<% else %>down<% end %>"><%= visits %></td>
			</tr>
			<tr>
				<td>New Visits</td>
				<td><%= results_two[3]["newVisits"] %></td>
				<% newVisits = results_two[3]["newVisits"] - results_three[3]["newVisits"]%>
				<td class="<% if newVisits > 0 %> up<% else %>down<% end %>"><%= newVisits %></td>
			</tr>
			<tr>
				<td>Avg. Time on Site</td>
				<td><%= results_two[2]["avgTimeOnSite"] %> sec</td>
				<% avgTimeOnSite = results_two[2]["avgTimeOnSite"] - results_three[2]["avgTimeOnSite"]%>
				<td class="<% if avgTimeOnSite > 0 %> up<% else %>down<% end %>"><%= avgTimeOnSite %> sec</td>
			</tr>
			<tr>
				<td>Impressions</td>
				<td><%= results_two[6]["impressions"] %></td>
				<% impressions = results_two[6]["impressions"] - results_three[6]["impressions"]%>
				<td class="<% if impressions > 0 %> up<% else %>down<% end %>"><%= impressions %></td>
			</tr>
				<td>Ad Clicks</td>
				<td><%= results_two[7]["adClicks"] %></td>
				<% clicks = results_two[7]["adClicks"] - results_three[7]["adClicks"]%>
				<td class="<% if clicks > 0 %> up<% else %>down<% end %>"><%= clicks %></td>
			</tr>
				<td>Click Through Rate</td>
				<td><%= sprintf("%.2f", results_two[7]["adClicks"]/results_three[6]["impressions"]*100) %>%</td>
				<% ctr = sprintf("%.2f", results_two[7]["adClicks"]/results_three[6]["impressions"]*100) - sprintf("%.2f", results_two[7]["adClicks"]/results_three[6]["impressions"]*100)%>
				<td class="<% if ctr > 0 %> up<% else %>down<% end %>"><%= ctr %>%</td>
			</tr>
		</table>
	<div class="col3">
	</div>
	<% else %>
	<div class="col2">
			<h2>Sorry! Your information is not ready yet.</h2>
			<p>Information is gathered once every hour and your app hasn't gathered any data yet.</p>
		</div>
	<% end %>
